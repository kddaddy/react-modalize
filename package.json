{
  "name": "react-modalized",
  "version": "0.1.0",
  "description": "App modalization package",
  "repository": {
    "type": "git",
    "url": "https://github.com/kddaddy/react-modalize.git"
  },
  "license": "MIT",
  "keywords": ["react", "modal", "modals", "javascript", "js"],
  "main": "dist/react-modalized.cjs.js",
  "module": "dist/react-modalized.esm.js",
  "browser": "dist/react-modalized.umd.js",
  "author": "kddaddy",
  "bugs": {
    "url": "https://github.com/kddaddy/react-modalized/issues"
  },
  "homepage": "https://github.com/kddaddy/react-modalized",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "node test/test.js",
    "pretest": "npm run build",
    "lint": "tslint 'lib/**/*.ts{,x}'",
    "lint:fix": "tslint --fix 'lib/**/*.ts{,x}'"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.0",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "jest": "^24.7.1",
    "rollup": "^1.16.7",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^23.10.5",
    "tslib": "^1.10.0",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-import-rules": "0.3.0",
    "tslint-no-circular-imports": "^0.6.2",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "files": ["dist"],
  "type": "types",
  "jest": {
    "roots": ["<rootDir>/src"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": ["ts", "tsx", "json", "node"]
  }
}
