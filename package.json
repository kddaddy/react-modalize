    "@types/react-dom": "^16.8.4",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "^24.7.1",
    "postcss-loader": "^3.0.0",
