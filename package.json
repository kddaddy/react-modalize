{
  "name": "react-modalized",
  "version": "0.0.1",
  "description": "React modals library",
  "repository": {
    "type": "git",
    "url": "https://github.com/komsikov/react-modalized"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "modal",
    "modals",
    "javascript",
    "js"
  ],
  "type": "module",
  "main": "dist/index.es.js",
  "module": "dist/index.sjs.js",
  "browser": "dist/index.umd.js",
  "types": "dist/lib/index.d.ts",
  "author": "bogeyboy",
  "bugs": {
    "url": "https://github.com/komsikov/react-modalized/issues"
  },
  "homepage": "https://github.com/kddaddy/react-modalized",
  "scripts": {
    "build:examples": "export ROLLUP_BUILD=examples; rollup -c",
    "build:lib": "export ROLLUP_BUILD=lib; rollup -c",
    "dev": "export ROLLUP_BUILD=examples; rollup -c -w",
    "test": "jest",
    "lint": "tslint '(lib|examples)/**/*.ts{,x}'",
    "lint:fix": "tslint --fix '(lib|test)/**/*.ts{,x}'",
    "prebuild": "./bin/prebuild",
    "postbuild": "./bin/postbuild"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "jest": "^29.5.0",
    "rollup": "^3.20.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-import-rules": "0.3.0",
    "tslint-no-circular-imports": "^0.7.0",
    "tslint-react": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "roots": [
      "<rootDir>/lib"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ]
  }
}
